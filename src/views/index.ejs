<section id="form">
  <div class="wrap">
    <h2>CASH OFFER IN 24 HOURS OR LESS!</h2>
    <form id="myform" action="/send-email" method="POST">
      <input class="inputFormFirst" type="text" id="fname" name="message" placeholder="Address" required><br>
      <div class="divFlexForm">
        <input class="inputForm" type="text" id="lname" name="name" placeholder="Name" required><br>
        <input class="inputForm" type="text" id="lphone" name="phone" placeholder="PhoneNumber" required><br>
      </div>
      <input class="inputFormFirst" type="email" id="fmail" name="email" placeholder="Email" required><br><br>
      <button class="btn" onclick="formsend()"  type="submit">
        <p class="parrBtn">Click here for an offer</p>
      </button>
    </form>
  </div>
</section>
